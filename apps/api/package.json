{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "covers:cache": "tsx scripts/cache-covers.ts",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.664.0",
    "@prisma/client": "^7.2.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.4",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^10.0.0",
    "aws-jwt-verify": "^4.0.1",
    "config": "workspace:*",
    "db": "workspace:*",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "fastify-plugin": "^4.5.1",
    "pino": "^9.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
